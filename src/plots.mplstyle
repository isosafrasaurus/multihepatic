# plots.mplstyle

figure.figsize    : 8, 6
figure.facecolor  : white
figure.autolayout : True

axes.titlesize    : 16
axes.labelsize    : 14
axes.grid         : True
axes.linewidth    : 1.0

lines.linewidth   : 2.0
image.cmap        : viridis

xtick.labelsize   : 12
ytick.labelsize   : 12

savefig.dpi       : 150
savefig.format    : png